

<% @clubs.each do |club| %>
  <% if club.shows.future.present? %>
  <h2><%= club.name %> </h2>

  <% club.shows.future.each do |show| %>
  <h4><%= show.title %> </h4>
  <h5><%= show.pretty_start_time %></h5>
  <% show.comedians.each do |comedian| %>
    <%= comedian.name %>

      <% end %>
    <% end %>
  <%- end -%>
<%- end -%>
